{
  "name": "xl-vision",
  "version": "0.0.2-beta.71",
  "description": "the high quality UI library for react",
  "author": "Rhys Xia",
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "libaray"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xl-vision/xl-vision.git"
  },
  "bugs": {
    "url": "https://github.com/xl-vision/xl-vision/issues"
  },
  "homepage": "https://xl-vision.github.io/xl-vision/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "lint:es": "xl-scripts lint:es --dirs src,site,script",
    "lint:style": "xl-scripts lint:style",
    "lint": "npm run lint:es && npm run lint:style",
    "compile:es": "xl-scripts compile:js --isEs --dest es && xl-scripts compile:style --dest es",
    "compile:lib": "xl-scripts compile:js && xl-scripts compile:style --dest lib",
    "compile": "npm run compile:es && npm run compile:lib",
    "dist": "xl-scripts bundle --styleEntry src/style/index.scss",
    "dev": "xl-scripts site --dev",
    "docs": "xl-scripts site --publicPath /xl-vision/",
    "test": "jest",
    "icon": "node script/icon/generate.js",
    "release": "node script/release.js",
    "prepublishOnly": "node script/checkPublish.js",
    "upgrade": "npm-check -u"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/format": "^8.2.0",
    "@types/classnames": "^2.2.9",
    "@types/clipboard": "^2.0.1",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.19",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "@xl-vision/icon-generator": "^0.0.1-beta.11",
    "@xl-vision/xl-tools-scripts": "^0.0.1-beta.43",
    "chalk": "^2.4.2",
    "clipboard": "^2.0.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.0.1",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "standard-version": "^7.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.12.0",
    "ts-jest": "^24.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "classnames": "^2.2.6",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
