{
  "name": "@xl-vision/xl-vision",
  "version": "0.0.2-beta.10",
  "description": "the high quality UI library for react",
  "author": "Rhys Xia",
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "libaray"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "js",
    "es",
    "style"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xl-vision/xl-vision.git"
  },
  "bugs": {
    "url": "https://github.com/xl-vision/xl-vision/issues"
  },
  "homepage": "https://xl-vision.github.io/xl-vision/",
  "scripts": {
    "compile:lib": "xl-vision-scripts compile:ts --modules js",
    "compile:es": "xl-vision-scripts compile:ts --modules es",
    "compile:style": "xl-vision-scripts compile:scss --src \"src/style/**/*.scss,!src/style/**/_*/**/*.scss\"",
    "compile": "npm run compile:lib && npm run compile:es && npm run compile:style",
    "dist": "xl-vision-scripts webpack",
    "tslint": "xl-vision-scripts tslint",
    "test": "xl-vision-scripts test",
    "prepublishOnly": "node script/publish.js",
    "updateSite": "node script/updateSite.js",
    "icon": "node script/icon/generate.js"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enquire.js": "^2.1.1",
    "@types/enzyme": "^3.1.14",
    "@types/node": "^11.9.4",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.8.3",
    "@xl-vision/icon-generator": "^0.0.1-beta.3",
    "@xl-vision/xl-vision-scripts": "^0.0.1-beta.8",
    "chalk": "^2.4.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.5",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "inquirer": "^6.2.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "semver": "^5.6.0",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "classnames": "^2.2.6",
    "enquire.js": "^2.1.6",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.8.2"
  },
  "sideEffects": [
    "dist/*"
  ]
}